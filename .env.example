# Google Calendar API Configuration
# NEVER commit actual values - use environment variables in production
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
GOOGLE_CALENDAR_ID=primary

# Admin Authentication
ADMIN_USERNAME=admin
# Generate password hash using: node -e "console.log(require('crypto').createHash('sha256').update('YOUR_PASSWORD_HERE' + 'YOUR_SALT_HERE').digest('hex'))"
ADMIN_PASSWORD_HASH=
ADMIN_SALT=generate_random_salt_here

# Email Configuration (for FormSubmit obfuscation)
FORM_SUBMIT_ENDPOINT=your_formsubmit_endpoint_here

# Security Keys
SESSION_SECRET=generate_random_32_char_string

# Cloudflare Turnstile (if using)
PUBLIC_TURNSTILE_SITE_KEY=
TURNSTILE_SECRET_KEY=
CLOUDFLARE_TURNSTILE_SITE_KEY=
CLOUDFLARE_TURNSTILE_SECRET_KEY=

# IP Security
ALLOWED_IPS=
BLOCKED_IPS=

# Environment
NODE_ENV=production

# IMPORTANT SECURITY NOTES:
# 1. NEVER commit the .env file to version control
# 2. Always use environment variables in production (Netlify, Vercel, etc.)
# 3. Rotate all keys immediately if exposed
# 4. Use secret management services (AWS Secrets Manager, HashiCorp Vault, etc.)
# 5. Enable audit logging for all secret access
# 6. The private key MUST be kept secret - exposure requires immediate rotation